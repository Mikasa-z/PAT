#include <iostream>
using namespace std;
int main(){
    double a[22],b[22],c[42]={0};
    int k1,k2,k3=0,ss=0;
    cin>>k1;
    for(int i=0;i<2*k1;i+=2) cin >> a[i] >> a[i+1];
    cin>>k2;
    for(int i=0;i<2*k2;i+=2) cin >> b[i] >> b[i+1];
    int i=0,j=0;
    while(i<2*k1&&j<2*k2){
        if(a[i]>b[j]) {
            c[k3]=a[i];
            c[k3+1]=a[i+1];
            i+=2;
            k3+=2;
        }else if(a[i]<b[j]){
            c[k3]=b[j];
            c[k3+1]=b[j+1];
            j+=2;
            k3+=2;
        }else {
            c[k3]=b[j];
            c[k3+1]=b[j+1]+a[i+1];
            i+=2;
            j+=2;
            k3+=2;
        }
    }
    if(i<2*k1){
        while(i<2*k1){
            c[k3]=a[i];
            c[k3+1]=a[i+1];
            i+=2;
            k3+=2;
        }
    }
    if(j<2*k2){
        while(j<2*k2){
            c[k3]=b[j];
            c[k3+1]=b[j+1];
            j+=2;
            k3+=2;
        }
    }
    for(int k=0;k<k3;k+=2){
        if(c[k+1]!=0) ss++;
    }
    printf("%d",ss);
    for(int k=0;k<k3;k+=2){
        if(c[k+1]!=0) printf(" %.0f %.1f",c[k],c[k+1]);
    }
    return 0;
}
